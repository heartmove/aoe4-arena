function Delhi_GetCost(bpName) 
	local time = delhi_upgrade_time[bpName]
	if time == nil then 
		time = delhi_upgrade_time[bpName.."_sul"]
	end
	if time ~= nil then
		
		return math.min(math.ceil(time * 0.9), time - 35)
	end
	
	return 0
end
